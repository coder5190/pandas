import pandas as pd

df1 = pd.DataFrame({
  'Product ID': [1, 2, 3, 4],
  'Product Name': ['t-shirt', 't-shirt', 'skirt', 'skirt'],
  'Color': ['blue', 'green', 'red', 'black']
})

print(df1)


#result
  Color	Product ID	Product Name
0	blue	   1	     t-shirt
1	green   2	       t-shirt
2	red	     3	     skirt
3	black	   4	     skirt
